<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FiveRingsActivity">

    <Spinner
        android:id="@+id/spinner_5_2"
        android:layout_width="@dimen/width"
        android:layout_height="@dimen/height"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="490dp"
        app:layout_constraintBottom_toTopOf="@+id/btn_BackFiveRings"
        app:layout_constraintEnd_toStartOf="@+id/spinner_5_3"
        app:layout_constraintStart_toEndOf="@+id/spinner_5_1"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner_5_1"
        android:layout_width="@dimen/width"
        android:layout_height="@dimen/height"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_5_2"
        app:layout_constraintEnd_toStartOf="@+id/spinner_5_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spinner_5_2" />

    <Spinner
        android:id="@+id/spinner_5_3"
        android:layout_width="@dimen/width"
        android:layout_height="0dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="37dp"
        app:layout_constraintBottom_toTopOf="@+id/btn_5"
        app:layout_constraintEnd_toStartOf="@+id/spinner_5_4"
        app:layout_constraintStart_toEndOf="@+id/spinner_5_2"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner_5_4"
        android:layout_width="@dimen/width"
        android:layout_height="@dimen/height"
        android:layout_marginEnd="38dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_5_2"
        app:layout_constraintEnd_toStartOf="@+id/spinner_5_5"
        app:layout_constraintStart_toEndOf="@+id/spinner_5_3"
        app:layout_constraintTop_toTopOf="@+id/spinner_5_2" />

    <Spinner
        android:id="@+id/spinner_5_5"
        android:layout_width="@dimen/width"
        android:layout_height="@dimen/height"
        android:layout_marginEnd="57dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_5_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/spinner_5_4"
        app:layout_constraintTop_toTopOf="@+id/spinner_5_4" />

    <Button
        android:id="@+id/btn_BackFiveRings"
        android:layout_width="@dimen/btn_width"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="14dp"
        android:text="@string/btn_back"
        app:icon="@drawable/abc_vector_test"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_5_2" />

    <Button
        android:id="@+id/btn_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="34dp"
        android:text="Button"
        android:onClick="onClick"
        app:layout_constraintBottom_toTopOf="@+id/tv_5_1"
        app:layout_constraintEnd_toEndOf="@+id/spinner_5_4"
        app:layout_constraintTop_toBottomOf="@+id/spinner_5_3" />

    <TextView
        android:id="@+id/tv_5_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="24dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/tv_5_2"
        app:layout_constraintStart_toStartOf="@+id/btn_5"
        app:layout_constraintTop_toBottomOf="@+id/btn_5" />

    <TextView
        android:id="@+id/tv_5_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="373dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv_5_1"
        app:layout_constraintTop_toBottomOf="@+id/tv_5_1" />

</androidx.constraintlayout.widget.ConstraintLayout>