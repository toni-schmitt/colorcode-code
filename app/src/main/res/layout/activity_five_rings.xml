<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FiveRingsActivity">

    <Spinner
        android:id="@+id/spinner_5_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_5_3"
        app:layout_constraintEnd_toStartOf="@+id/spinner_5_3"
        app:layout_constraintStart_toEndOf="@+id/spinner_5_1"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner_5_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_5_2"
        app:layout_constraintEnd_toStartOf="@+id/spinner_5_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spinner_5_2" />

    <Spinner
        android:id="@+id/spinner_5_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_5_4"
        app:layout_constraintEnd_toStartOf="@+id/spinner_5_4"
        app:layout_constraintStart_toEndOf="@+id/spinner_5_2"
        app:layout_constraintTop_toTopOf="@+id/spinner_5_4" />

    <Spinner
        android:id="@+id/spinner_5_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/btn_Continue_FiveRings"
        app:layout_constraintEnd_toStartOf="@+id/spinner_5_5"
        app:layout_constraintStart_toEndOf="@+id/spinner_5_3"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner_5_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="57dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_5_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/spinner_5_4"
        app:layout_constraintTop_toTopOf="@+id/spinner_5_4"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_BackFiveRings"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginBottom="15dp"
        android:text="@string/btn_back"
        app:icon="@drawable/abc_vector_test"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_5_2"
        app:layout_constraintVertical_bias="0.957" />

    <Button
        android:id="@+id/btn_Continue_FiveRings"
        android:layout_width="@dimen/btn_width"
        android:layout_height="@dimen/btn_height"
        android:layout_marginBottom="452dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_5_4"
        app:layout_constraintVertical_bias="0.088" />

    <TextView
        android:id="@+id/tv_Resistance_FiveRings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="TextView"
        app:layout_constraintEnd_toStartOf="@+id/tv_Tolerance_FiveRings"
        app:layout_constraintHorizontal_bias="0.724"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Continue_FiveRings" />

    <TextView
        android:id="@+id/tv_Tolerance_FiveRings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="140dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/btn_BackFiveRings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Continue_FiveRings"
        app:layout_constraintVertical_bias="0.088" />

</androidx.constraintlayout.widget.ConstraintLayout>